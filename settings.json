{
  "workbench.colorTheme": "Tokyo Night AltSynt",
  "workbench.startupEditor": "newUntitledFile",
  "workbench.iconTheme": "material-icon-theme",
  "workbench.editor.languageDetection": true,
  "workbench.colorCustomizations": {
    "editorOverviewRuler.background": "#1d1f2d",
    "editorCursor.foreground": "#1e95eb"
  },
  "editor.codeActionsOnSave": {
    "source.fixAll.eslint": true
  },
  "editor.suggestSelection": "first",
  "editor.guides.bracketPairs": "active",
  "editor.bracketPairColorization.enabled": true,
  "editor.fontFamily": "'Fira Code', 'Cascadia Code', Consolas, 'Courier New', monospace",
  "editor.fontLigatures": "true",
  "editor.fontWeight": "500",
  "editor.autoClosingBrackets": "always",
  "editor.parameterHints.enabled": true,
  "editor.formatOnSave": true,
  "errorLens.removeLinebreaks": false,
  "errorLens.fontWeight": "300",
  "errorLens.fontStyleItalic": true,
  "errorLens.fontSize": "12",
  "errorLens.messageMaxChars": 50,
  "editor.unicodeHighlight.ambiguousCharacters": false,
  "errorLens.enabledDiagnosticLevels": ["info", "error", "warning"],
  "telemetry.telemetryLevel": "off",
  "todo-tree.general.tags": [
    "BUG:",
    "HACK:",
    "FIXME:",
    "TO FIX:",
    "TODO:",
    "XXX:",
    "MISSING:",
    "BUG",
    "HACK",
    "FIXME",
    "TO FIX",
    "TODO",
    "XXX",
    "MISSING",
    "[ ]",
    "[x]"
  ],
  "todo-tree.highlights.customHighlight": {
    "BUG:": {
      "icon": "bug"
    },
    "BUG": {
      "icon": "bug"
    },
    "FIXME:": {
      "icon": "flame"
    },
    "FIXME": {
      "icon": "flame"
    },
    "TO FIX:": {
      "icon": "flame"
    },
    "TO FIX": {
      "icon": "flame"
    }
  },
  "[css]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "color-highlight.markerType": "dot-before",
  "color-highlight.languages": ["!css", "!html", "!scss", "*"],
  "liveServer.settings.donotShowInfoMsg": true,
  "files.exclude": {
    "**/.classpath": true,
    "**/.project": true,
    "**/.settings": true,
    "**/.factorypath": true
  },
  "workbench.editor.enablePreview": true,
  "launch": {
    "configurations": [],
    "compounds": []
  },
  "todo-tree.highlights.highlightDelay": 400,
  "todo-tree.filtering.excludeGlobs": ["**/node_modules/**"],
  "[javascript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.wordWrap": "off",
  "todo-tree.regex.regex": "(//|#|<!--|;|/\\*|^|^\\s*(-|\\d+.))\\s*($TAGS)",
  "editor.tabSize": 2,
  "emmet.syntaxProfiles": {
    "javascript": "html"
  },
  "emmet.includeLanguages": {
    "javascript": "javascriptreact",
    "typescript": "typescriptreact"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "prettier.jsxSingleQuote": true,
  "prettier.singleQuote": true,
  "terminal.integrated.tabs.enabled": true,
  "terminal.integrated.defaultProfile.windows": "Git Bash",
  "[javascriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "[jsonc]": {
    "editor.quickSuggestions": {
      "strings": true
    },
    "editor.suggest.insertMode": "replace",
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.formatOnSave": true
  },
  "explorer.compactFolders": false,
  "security.workspace.trust.untrustedFiles": "open",

  "redhat.telemetry.enabled": false,
  "[yaml]": {
    "editor.defaultFormatter": "redhat.vscode-yaml"
  },
  "git.suggestSmartCommit": false,
  "files.associations": {
    "*.css": "css"
  },
  "oneDarkPro.bold": true,
  "oneDarkPro.italic": true,
  "oneDarkPro.vivid": true,
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },

  "workbench.editorAssociations": {
    "git-rebase-todo": "default"
  },
  "files.autoSaveDelay": 20000,
  "files.eol": "\n",
  "gitlens.hovers.currentLine.over": "line",
  "window.menuBarVisibility": "classic",
  "editor.tokenColorCustomizations": {
    "textMateRules": [
      {
        "scope": [
          //following will be in italic (=FlottFlott)
          "comment",
          "entity.name.type.class", //class names
          "keyword", //import, export, return…
          "constant", //String, Number, Boolean…, this, super
          "storage.modifier", //static keyword
          "storage.type.class.js" //class keyword
        ],
        "settings": {
          "fontStyle": "italic"
        }
      },
      {
        "scope": [
          //following will be excluded from italics (VSCode has some defaults for italics)
          "invalid",
          "keyword.operator",
          "constant.numeric.css",
          "keyword.other.unit.px.css",
          "constant.numeric.decimal.js",
          "constant.numeric.json"
        ],
        "settings": {
          "fontStyle": ""
        }
      }
    ]
  },
  "editor.inlineSuggest.enabled": true,
  "github.copilot.enable": {
    "*": true,
    "yaml": false,
    "plaintext": true,
    "markdown": false,
    "javascript": true
  },
  "editor.cursorBlinking": "expand",
  "editor.cursorWidth": 2,
  "workbench.activityBar.visible": false,
  "activitusbar.showSourceControlCounter": true,
  "activitusbar.views": [
    {
      "name": "explorer",
      "codicon": "explorer-view-icon"
    },
    {
      "name": "search",
      "codicon": "search-view-icon"
    },
    {
      "name": "scm",
      "codicon": "source-control-view-icon"
    },
    {
      "name": "debug",
      "codicon": "run-view-icon"
    },
    {
      "name": "extensions",
      "codicon": "extensions-view-icon"
    },
    {
      "name": "extension.todo-tree-container",
      "codicon": "tasklist",
      "tooltip": "Todos"
    },
    {
      "codicon": "kebab-vertical"
    }
  ],
  "cSpell.language": "en,es-ES"
}
